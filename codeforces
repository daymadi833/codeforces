    <div class="new-comments-box" data-position="outside" data-index="-1" style="display: none;">
        <div class="up dir" title="Ctrl+вверх">&uarr;<hr/>
        </div>
        <div class="info" title="Новые комментарии"></div>
        <div class="down dir" title="Ctrl+вниз">
            <hr/>&darr;
